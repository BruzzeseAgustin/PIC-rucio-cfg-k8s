[client]
rucio_host = {{ RUCIO_CFG_RUCIO_HOST | default('localhost') }}
auth_host = {{ RUCIO_CFG_AUTH_HOST | default('localhost') }}
ca_cert = {{ RUCIO_CFG_CA_CERT | default('/etc/pki/tls/certs/ca-bundle.crt') }}
auth_type = {{ RUCIO_CFG_AUTH_TYPE | default('userpass') }}
username = {{ RUCIO_CFG_USERNAME | default('ddmlab') }}
password = {{ RUCIO_CFG_PASSWORD | default('secret') }}
account = {{ RUCIO_CFG_ACCOUNT | default('root') }}
client_cert = {{ RUCIO_CFG_CLIENT_CERT | default('') }}
client_key = {{ RUCIO_CFG_CLIENT_KEY | default('') }}
client_x509_proxy = {{ RUCIO_CFG_CLIENT_X509_PROXY | default('') }}
request_retries = {{ RUCIO_CFG_REQUEST_RETRIES | default('3') }}
